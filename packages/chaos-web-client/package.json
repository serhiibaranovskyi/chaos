{
  "name": "chaos-web-client",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --plugin-search-dir=. --write .",
    "lint": "eslint --ext svelte,ts,js,cjs .",
    "prepare": "pnpm run smui-theme-light && pnpm run smui-theme-dark",
    "preview": "vite preview",
    "smui-theme-light": "smui-theme compile static/smui.css -i src/theme -i ../../node_modules",
    "smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark -i ../../node_modules",
    "test": "pnpm run check"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.5.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.48.1",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.8.1",
    "smui-theme": "7.0.0-beta.0",
    "svelte": "^3.54.0",
    "svelte-check": "^3.0.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3",
    "vite": "^4.0.0"
  },
  "type": "module",
  "dependencies": {
    "@material/typography": "^14.0.0",
    "@smui/button": "7.0.0-beta.0",
    "@smui/common": "7.0.0-beta.0",
    "@smui/top-app-bar": "7.0.0-beta.0",
    "@tanstack/svelte-query": "^4.24.4"
  }
}
